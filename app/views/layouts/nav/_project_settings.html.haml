    = nav_link(controller: :triggers) do
      = link_to namespace_project_triggers_path(@project.namespace, @project), title: '触发器' do
        %span
          触发器
    = nav_link(controller: :pipelines_settings) do
      = link_to namespace_project_pipelines_settings_path(@project.namespace, @project), title: 'CI/CD 流水线' do
        %span
